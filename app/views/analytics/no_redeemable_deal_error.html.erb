<%= render 'layouts/biz/dash' %>
<div class="contentWrapper">
  <div class="content">

    <!--Flash messages-->
    <% flash.each do |key, value| %>
        <div class="alert alert-<%= key %>"><%= value %></div>
    <% end %>

    <!-- Content Heading -->
    <div class="pull-left">
      <p class="b2 content-title">Deal Analytics</p>
    </div>

    <div class="clearfix"></div>
    <div class="content-separator"></div>

    <!-- Content Body -->
    <div class = "well col-lg-6 col-lg-offset-3" style="text-align: center; font-weight: bold">
      <p>Sorry, you do not have any redeemable deals. This prevents us from displaying Popularity of Venues and Popularity of Deals in Venues chart.
      You can list a redeemable deal <%= link_to 'here', new_deal_path %>. Do remember to enable the redeemable option to allow data to be gathered.</p>
    </div>

  </div><!-- /content -->
</div><!-- /contentWrapper -->

<%= content_for :lean_overlay do %>
<% end %>

<%= content_for :body_footer do %>
<% end %>