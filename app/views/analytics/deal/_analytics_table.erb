<div class="col-lg-12">
  <div class="panel-group" id="accordion">
    <div class="panel panel-primary">
      <div class="panel-heading panel-heading-custom">
        <h4 class="panel-title">
          <a data-toggle="collapse" href="#collapseOne">
            Active Deals
            <span class="glyphicon glyphicon-triangle-bottom"></span>
          </a>
        </h4>
      </div>
      <div id="collapseOne" class="panel-collapse collapse in">
        <div class="table-responsive">
          <table class="table table-bordered table-hover">
            <% if !@deal_analytics_in_table[0].blank? %>
                <tr>
                  <th>Title</th>
                  <th style="text-align: center; width: 15%">% of wishlisters that have viewed the deal</th>
                  <th style="text-align: center; width: 15%">% of users that redeem the deal after viewing it</th>
                  <th style="text-align: center; width: 15%">Percentage/Number of users who redeem more than once</th>
                  <th style="text-align: center; width: 15%">Average number of redemptions for users who redeemed
                    multiple times
                  </th>
                  <th style="text-align: center; width: 15%">Average time span between consecutive redemptions by a
                    user
                  </th>
                  <th colspan="1"></th>
                </tr>

                <% @deal_analytics_in_table[0].each do |deal| %>
                    <tr>
                      <td><%= deal[0].title %></td>
                      <% if @payment.add_on1 && deal[0].pushed %>
                          <% wishlist_view = deal[1] %>
                          <% if wishlist_view == 'N/A' %>
                              <td style="text-align: center;">
                                <p id="untracable" data-toggle="tooltip" data-placement="top" title="As you have no wishlisters when you pushed your deal, data cannot be gathered">N/A</p>
                              </td>
                          <% else %>
                              <td style="text-align: center;"><%= wishlist_view %>%</td>
                          <% end %>
                      <% elsif @payment.add_on1 && !deal[0].pushed %>
                          <td style="text-align: center;">
                            <p id="untracable" data-toggle="tooltip" data-placement="top" title="Deal has not been pushed hence, data cannot be gathered">N/A</p>
                          </td>
                      <% else %>
                          <td style="text-align: center;">
                            <p id="untracable" data-toggle="tooltip" data-placement="top" title="You have not purchased the Push Notification addon, hence you cannot view this">N/A</p>
                          </td>
                      <% end %>

                      <% if deal[0].redeemable && (deal[0].start_date <= Date.today) %>
                          <% percentage_views = deal[2] %>
                          <% if percentage_views == 'No Views Yet' %>
                              <td style="text-align: center;"> <%= percentage_views %></td>
                          <% else %>
                              <td style="text-align: center;"> <%= percentage_views %>%</td>
                          <% end %>
                          <% if deal[3] == 'No Redeems Yet' %>
                              <td style="text-align: center;"> <%= deal[3] %></td>
                          <% else %>
                              <td style="text-align: center;"> <%= deal[3] %>% = <%= deal[4] %> Users</td>
                          <% end %>
                          <td style="text-align: center;"> <%= deal[5] %></td>
                          <td style="text-align: center;"> <%= deal[6] %></td>
                      <% elsif deal[0].redeemable && (deal[0].start_date > Date.today) %>
                          <td style="text-align: center;">
                            <p id="untracable" data-toggle="tooltip" data-placement="top" title="Deal has not started yet hence, data cannot be gathered">N/A</p>
                          </td>
                          <td style="text-align: center;">
                            <p id="untracable" data-toggle="tooltip" data-placement="top" title="Deal has not started yet hence, data cannot be gathered">N/A</p>
                          </td>
                          <td style="text-align: center;">
                            <p id="untracable" data-toggle="tooltip" data-placement="top" title="Deal has not started yet hence, data cannot be gathered">N/A</p>
                          </td>
                          <td style="text-align: center;">
                            <p id="untracable" data-toggle="tooltip" data-placement="top" title="Deal has not started yet hence, data cannot be gathered">N/A</p>
                          </td>
                      <% else %>
                          <td>
                            <p id="untracable" data-toggle="tooltip" data-placement="top" title="Deal is not redeemable hence, data cannot be gathered">N/A</p>
                          </td>
                          <td>
                            <p id="untracable" data-toggle="tooltip" data-placement="top" title="Deal is not redeemable hence, data cannot be gathered">N/A</p>
                          </td>
                          <td>
                            <p id="untracable" data-toggle="tooltip" data-placement="top" title="Deal is not redeemable hence, data cannot be gathered">N/A</p>
                          </td>
                          <td>
                            <p id="untracable" data-toggle="tooltip" data-placement="top" title="Deal is not redeemable hence, data cannot be gathered">N/A</p>
                          </td>
                      <% end %>
                      <td>
                        <%= link_to 'Show Deal', deal_path(deal[0]), class: "btn btn-xs btn-success" %>
                      </td>
                    </tr>
                <% end %>
            <% else %>
                <div class="well col-lg-12" style="text-align: center; font-weight: bold">
                  <p>Oops! You do not have any active deals!</p>
                </div>
            <% end %>
          </table>
        </div>
      </div>
    </div>
