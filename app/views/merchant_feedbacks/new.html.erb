<%= render 'layouts/biz/dash' %>

<div class="contentWrapper">
  <div class="content">

    <!-- Flash Error Message -->
    <% if @merchant_feedback.errors.any? %>
        <div id="error_explanation" class="alert alert-error">
          <h2>
            <%= pluralize(@merchant_feedback.errors.count, "error") %> prohibited this venue from being saved:
          </h2>
          <ul>
            <% @merchant_feedback.errors.full_messages.each do |msg| %>
                <li><%= msg %></li>
            <% end %>
          </ul>
        </div>
    <% end %>

    <div class="form-style-10" style="max-width:60%">
      <h1> Submit Your Feedback!</h1>
      <%= render :partial => 'form/merchant_feedback_create_form' %>
    </div>

  </div>
</div>

<%= content_for :form_footer do %>
    <%= render :partial => 'layouts/form_style' %>
<% end %>