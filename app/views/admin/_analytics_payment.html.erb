<h2>Plans</h2>
<div id="plan_subscribers" style="height: 400px; width:600px; float: left;"></div>
<div id="plan_payments" style="height: 400px; width:600px; float: left;"></div>

<footer>
  <script src="http://code.highcharts.com/highcharts.js"></script>
  <script src="http://code.highcharts.com/highcharts-3d.js"></script>
  <script src="http://code.highcharts.com/modules/exporting.js"></script>
  <script>
      $(function () {
          $('#plan_subscribers').highcharts({
              credits: {
                  enabled: false
              },
              chart: {
                  type: 'pie',
                  options3d: {
                      enabled: true,
                      alpha: 45,
                      beta: 0
                  }
              },
              title: {
                  text: 'Popularity of All Plans'
              },
              subtitle: {
                  text: 'Number of Subscribers'
              },
              tooltip: {
                  pointFormat: '{series.name}: <b>{point.percentage:.1f}%</b>'
              },
              plotOptions: {
                  pie: {
                      allowPointSelect: true,
                      cursor: 'pointer',
                      depth: 35,
                      dataLabels: {
                          enabled: true,
                          format: '{point.name} {point.y}'
                      }
                  }
              },
              series: [{
                  type: 'pie',
                  name: 'Subscribers',
                  data: [
                      ['Basic Service',  <%= plan_all %>],
                      ['Deal Listing Service',  <%= plan1 %>]
                  ]
              }]
          });
      });
      $(function () {
          $('#plan_payments').highcharts({
              credits: {
                  enabled: false
              },
              chart: {
                  type: 'pie',
                  options3d: {
                      enabled: true,
                      alpha: 45,
                      beta: 0
                  }
              },
              title: {
                  text: 'Revenue from All Plans'
              },
              subtitle: {
                  text: 'Contribution of each service to total revenue'
              },
              tooltip: {
                  pointFormat: '{series.name}: <b>{point.percentage:.1f}%</b>'
              },
              plotOptions: {
                  pie: {
                      allowPointSelect: true,
                      cursor: 'pointer',
                      depth: 35,
                      dataLabels: {
                          enabled: true,
                          format: '{point.name} ${point.y}'
                      }
                  }
              },
              series: [{
                  type: 'pie',
                  name: 'Subscribers',
                  data: [
                      ['Basic Service',  0],
                      ['Deal Listing Service',  <%= plan1_cost %>]
                  ]
              }]
          });
      });
  </script>
</footer>